ius": 2, "name": "Bob", "x": 88, "DEFAULT_labelColor": "#0d0d0d", "PSO": ["read", "write", "delete"], "userData": {}, "types": {"data": []}, "alpha": 1, "DEFAULT_rsColor": "#0d0d0d", "cssClass": "ActorUI2"}, {"alpha": 1, "userData": {}, "x": 98, "policy": "CLAUSE alice_policy (\n    // access control\n    (FORALL c:Customer FORALL d:data \n        IF (d.subject==c) THEN {\n            PERMIT c.read[Alice](d) AND \n            PERMIT c.write[Alice](d) AND \n            PERMIT c.delete[Alice](d)\n        }\n    )\n    AND\n    (FORALL a:Actor FORALL d:UserName \n        IF ((a==NSA) OR @Employee(a)) THEN { \n            DENY a.read[Alice](d)\n        }\n    ) \n    AND \n    (FORALL a:Actor FORALL d:DisplayName \n        IF ((a==NSA) OR (@Employee(a))) THEN {\n            PERMIT a.read[Alice](d)\n        }\n    )\n    // data transfer \n \tAND (FORALL d:Sensitive FORALL a:Actor \n \t    IF (@Europe(a)) THEN {PERMIT Alice.transfer[a](d)}) \n\t\n\tAND (FORALL d:Sensitive FORALL a:Actor \n\t    IF (NOT @Europe(a)) THEN {DENY Alice.transfer[a](d)}) \n\n    // data retention \n    AND (FORALL ds:Sensitive  MUST( Alice.delete[Alice](ds) BEFORE \"6 month\"))\n    \n    AUDITING MUST(dpa.audit[Alice](log) BEFORE \"1 month\")\n    IF_VIOLATED_THEN MUST(dpa.sanction[Alice](d))\n )", "tlabeltxt": "alice_policy", "id": "6017d259-d2ad-0e34-8c4e-a16c0b012cd7", "radius": 0, "bgColor": "#A0A0A0", "width": 100, "color": "#1B1B1B", "angle": 0, "y": 459, "ports": [{"name": "input0", "alpha": 1, "locator": "draw2d.layout.locator.InputPortLocator", "userData": {}, "color": "#1B1B1B", "id": "02bb8310-b1a0-36ce-1162-5330e3683d92", "maxFanOut": 9007199254740991, "bgColor": "#4F6870", "width": 10, "type": "draw2d.InputPort", "angle": 0, "height": 10, "port": "draw2d.InputPort", "stroke": 1, "dasharray": null, "cssClass": "draw2d_InputPort"}, {"name": "output0", "alpha": 1, "locator": "draw2d.layout.locator.OutputPortLocator", "userData": {}, "color": "#1B1B1B", "id": "24ca130d-c7b5-5303-b43b-