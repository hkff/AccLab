<filetype binary="false" default_extension="" description="TSPASS files" name="TSPASS">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="%" />
      <option name="COMMENT_START" value="" />
      <option name="COMMENT_END" value="" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="&amp;;-&gt;;&lt;-&gt;;&lt;=&gt;;=&gt;;not;|;~" ignore_case="true" />
    <keywords2 keywords="false;true" />
    <keywords3 keywords="always;next;sometime;unless;until" />
    <keywords4 keywords="!;?" />
  </highlighting>
  <extensionMap>
    <mapping ext="tspass" />
  </extensionMap>
</filetype>