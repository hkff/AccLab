<filetype binary="false" default_extension="" description="AAL files" name="AAL">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="/*" />
      <option name="COMMENT_END" value="*/" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords=",;after;and;before;exists;forall;if;not;onlywhen;or;then;where" ignore_case="true" />
    <keywords2 keywords="agent;apply;auditing;behavior;call;check;clause;data;exec;if_violated_then;load;macro;of;service;type;types" />
    <keywords3 keywords="always;must;mustnot;never;sometime;until" />
    <keywords4 keywords="aa;actions;attributes;deny;extends;get_audit;get_rectification;get_usage;permit;provided;ps;purpose;rc;required;rs;subject;uc" />
  </highlighting>
  <extensionMap>
    <mapping ext="aal" />
  </extensionMap>
</filetype>