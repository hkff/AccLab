      {"name": "", "id": "resource", "type": "text", "pre": " ", "post": " Resource"},
      {"name": "", "id": "owner", "type": "text", "pre": " ", "post": " Owner"},
      {"name": "", "id": "user", "type": "text", "pre": " ", "post": " User"}
    ],
	"aal": [
      "{{ (({period}.val != '')?'MUST( {owner}.val.delete[{owner}.val]({resource}.val) BEFORE {period}.val)':'') }}"
    ],
	"html": [
      "<div class='varGroupe'> {resource} </div>",
      "<div class='varGroupe'> {user} </div>",
      "<div class='varGroupe'> {owner} </div>",
      "<div class='varGroupe'> {period}</div> {genBtn}"
    ],
	"xacml": ""
}
