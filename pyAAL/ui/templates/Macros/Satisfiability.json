      {"name": "", "id": "type", "type": "select", "pre": " Type ", "post": "", "src": "[\"ue\", \"ae\", \"re\"]"}

    ],
	"aal": [
      "CALL {{ ({verbose}.checked)?'v':'' }}sat{{ ('{type}.val' == '')?'':'_'+'{type}.val'  }}(\"{c1}.val\")"
