	"html": [
      "<div class='varGroupe'>  <h4 class='varsHeader'>Clauses :</h4>  {c1}</div> ",
      "<div class='varGroupe'>  <h4 class='varsHeader'>Options :</h4> {verbose} {type}</div>",
      " {genBtn}"
    ],
	"xacml": ""
}
